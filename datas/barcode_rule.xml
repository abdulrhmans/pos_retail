<odoo>
    <data noupdate="0">



        <record id="barcode_rule_voucher" model="barcode.rule">
            <field name="name">Scan Voucher</field>
            <field name="barcode_nomenclature_id" ref="barcodes.default_barcode_nomenclature"/>
            <field name="sequence">87</field>
            <field name="type">voucher</field>
            <field name="encoding">ean13</field>
            <field name="pattern">999.*</field>
        </record>

        <record id="barcode_rule_return_order" model="barcode.rule">
            <field name="name">Return order</field>
            <field name="barcode_nomenclature_id" ref="barcodes.default_barcode_nomenclature"/>
            <field name="sequence">88</field>
            <field name="type">order</field>
            <field name="encoding">ean13</field>
            <field name="pattern">998.*</field>
        </record>

        <record id="barcode_rule_login_security" model="barcode.rule">
            <field name="name">Login Security</field>
            <field name="barcode_nomenclature_id" ref="barcodes.default_barcode_nomenclature"/>
            <field name="sequence">89</field>
            <field name="type">login_security</field>
            <field name="encoding">ean13</field>
            <field name="pattern">041</field>
        </record>

        <record id="barcode_rule_return_product" model="barcode.rule">
            <field name="name">Return Products</field>
            <field name="barcode_nomenclature_id" ref="barcodes.default_barcode_nomenclature"/>
            <field name="sequence">100</field>
            <field name="type">return_products</field>
            <field name="encoding">any</field>
            <field name="pattern">.*</field>
        </record>



    </data>
</odoo>
